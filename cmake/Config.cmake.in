# - Config file for the fformation package
# It defines the following variables
#  FFORMATION_INCLUDE_DIRS - include directories for fformation
#  FFORMATION_LIBRARIES    - libraries to link against
#  FFORMATION_EXECUTABLE   - the bar executable

# Compute paths
get_filename_component(FFORMATION_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
set(FFORMATION_INCLUDE_DIRS "@CONF_INCLUDE_DIRS@")

# Our library dependencies (contains definitions for IMPORTED targets)
if(NOT TARGET foo AND NOT FFORMATION_BINARY_DIR)
  include("${FFORMATION_CMAKE_DIR}/@PROJECT_NAME@Targets.cmake")
endif()

# These are IMPORTED targets created by fformationTargets.cmake
set(FFORMATION_LIBRARIES "@PROJECT_NAME@")
set(FFORMATION_EXECUTABLE "@APPLICATIONS@")
